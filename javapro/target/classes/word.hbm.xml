<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
  <class name="entities.Word" table="WORD">
   <id name="id" type="int" column="ID" >
   <generator class="increment"/>
  </id>
 
  <property name="word">
    <column name="WORD" />
  </property>
  <property name="meaning">
    <column name="MEANING"/>
  </property>
  <property name="wdate">
    <column name="WDATE" />
    <type name="timestamp"/>
  </property>
   <property name="wnotice">
    <column name="WNOTICE"/>
  </property>
 </class>
